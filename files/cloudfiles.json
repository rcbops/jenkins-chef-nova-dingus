{
    "cookbook_versions": {
    },
    "json_class": "Chef::Environment",
    "override_attributes": {
        "kong": {
            "swift_store_region": "DFW"
        },
        "osops": {
            "apply_patches": true
        },
        "nova": {
            "libvirt": {
                "virt_type": "qemu"
            },
            "networks": {
              "public": {
                "bridge_dev": "gretap.api",
                "ipv4_cidr": "192.168.100.0/24",
                "bridge": "br99",
                "dns1": "8.8.8.8",
                "dns2": "8.8.4.4",
                "label": "public"
              }
            }
        },
        "glance": {
            "image_upload": false,
            "api": {
                "default_store": "swift",
                "swift_store_user": "changeme",
                "swift_store_key": "changeme",
                "swift_store_auth_version": "2",
                "swift_store_auth_address": "https://identity.api.rackspacecloud.com/v2.0/",
                "swift_store_region": "DFW"
            },
            "images": [
                "cirros"
            ]
        },
        "mysql": {
            "allow_remote_root": true,
            "root_network_acl": "10.%"
        },
        "osops_networks": {
            "management": "10.0.0.0/8",
            "nova": "10.0.0.0/8",
            "swift": "10.0.0.0/8",
            "swift-lb": "10.0.0.0/8",
            "public": "10.0.0.0/8"
        }
    },
    "name": "cloudfiles",
    "chef_type": "environment",
    "description": ""
}
