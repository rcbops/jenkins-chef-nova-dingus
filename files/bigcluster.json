            ]
        },
        "glance": {
            "image_upload": false,
            "api": {
                "default_store": "swift"
            },
            "images": [
                "cirros"
            ]
        },
        "mysql": {
            "allow_remote_root": true,
            "root_network_acl": "10.%"
        },
        "osops_networks": {
            "management": "10.127.53.0/24",
            "nova": "10.0.0.0/8",
