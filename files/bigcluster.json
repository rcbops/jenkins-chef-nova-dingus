            }
        },
        "nova": {
            "config": {
                "log_verbosity": "DEBUG"
            },
            "network": {
                "multi_host": true
            },
            "apply_patches": true,
            "libvirt": {
                "virt_type": "qemu"
            },
            "networks": [
                {
                    "network_size": "255",
                    "bridge_dev": "gretap.api",
