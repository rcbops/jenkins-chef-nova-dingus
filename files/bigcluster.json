    },
    "json_class": "Chef::Environment",
    "override_attributes": {
        "monitoring": {
            "metric_provider": "collectd",
            "procmon_provider": "monit"
        },
        "horizon": {
          "theme": "Rackspace"
        },
        "vips": {
          "keystone-service-api": "127.0.0.2",
        },
        "swift": {
            "use_informant": false,
            "authkey": "3f281b71-ce89-4b27-a2ad-ad873d3f2760",
            "swift_hash": "107c0568ea84",
            "auto_rebuild_rings": true,
